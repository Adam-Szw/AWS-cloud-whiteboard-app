package whiteboard;

import java.util.ArrayList;
import java.util.List;

import whiteboard.drawings.Shape;

public class State {
	
	public List<Shape> shapes = new ArrayList<Shape>();
	
	public void append(Shape shape) {
		shapes.add(shape);
	}
	
	public String toString() {
		String str = "";
		for(Shape shape : shapes) {
			str += shape.toString() + "|";
		}
		return str;
	}
	
}
